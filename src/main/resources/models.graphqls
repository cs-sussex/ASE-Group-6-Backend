schema {
    query: Query
}

type Query {
    properties(postcode: String): [Property]
    property(id: String): Property
}

type Property {
    id: String
    paon: String
    saon: String
    postcode: String
    street: String
    locality: String
    town: String
    district: String
    county: String
    pricePaidData: [PricePaidData]
}

type PricePaidData {
    dateOfTransfer: String,
    pricePaid: Int
}